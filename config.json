{
  "allowed_commands": [
    "Get-ChildItem",
    "Get-Item", 
    "Get-Content",
    "Get-Location",
    "Set-Location",
    "Test-Path",
    "Get-Process",
    "Get-Service",
    "Get-Date",
    "Get-Host",
    "Write-Output",
    "Write-Host",
    "Select-Object",
    "Where-Object",
    "Sort-Object",
    "Measure-Object",
    "Format-Table",
    "Format-List",
    "Get-Help",
    "Get-Command",
    "Get-Member"
  ],
  "allowed_directories": [
    "C:\\Users\\Public",
    "C:\\temp",
    "C:\\Windows\\System32",
    "C:\\Program Files\\Common Files",
    "D:\\workspace"
  ],
  "blocked_patterns": [
    "[;&|`]",
    "Invoke-Expression",
    "Invoke-Command", 
    "iex\\s",
    "icm\\s",
    "Start-Process",
    "sps\\s",
    "Remove-Item",
    "rm\\s",
    "del\\s",
    "rmdir\\s",
    "\\.ps1",
    "\\.bat",
    "\\.cmd", 
    "\\.exe",
    "powershell\\.exe",
    "cmd\\.exe",
    "wget",
    "curl",
    "Invoke-WebRequest",
    "Invoke-RestMethod",
    "New-Object\\s+System\\.Net",
    "DownloadString",
    "DownloadFile",
    ">>",
    "Out-File"
  ],
  "max_command_length": 200,
  "timeout_seconds": 30
}